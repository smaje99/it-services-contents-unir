---
interface Props {
  class?: string;
  [key: string]: unknown; // props adicionales
}
const { class: className = '', ...rest } = Astro.props;
---

<header class:list={['dialog-header', className]} {...rest}>
  <slot />
</header>

<style>
  .dialog-header {
    display: grid;
    gap: 0.25rem;
  }
</style>
